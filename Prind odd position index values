#include <iostream>
using namespace std;
int main() {
    int arr[] = {2,4,3,8,7,10,9,5,90,3,4,7};
     int n = sizeof(arr) / sizeof(int);

     //loop to print odd places values of elements
     for(int i = 0; i<n; i++){
         if(i % 2 != 0){
         cout<<i<<"   ->  "<<arr[i]<<endl;
         }
     }
     
     return 0;                                                     
}
