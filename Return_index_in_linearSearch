#include <iostream>
using namespace std;

//  --------------linear search function return index no of targeted element
int linear_Search(int arr[], int target, int n){
    for(int i = 0; i<n; i++){
        if(arr[i] == target){
            cout<<"Element present at "<<i<<" position"<<endl;
            
            // break; ------ use this when we need to consider only first search.
        }
    }
return 0;
}

int main(){
    int n;
    cin>>n;
    int arr[n];

    for(int i = 0; i<n ; i++){
        cin>>arr[i];
    }

    for(int i = 0; i<n; i++){
        cout<<arr[i]<<endl;                  // print array
    }

    
    int target;
    cin>>target;

    linear_Search(arr,target,n);
    // cout<<ans;
    return 0;

}
