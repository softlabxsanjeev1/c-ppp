#include <iostream>
using namespace std;
int main() {
    int arr[5];          // decalaration of array

    arr[0] = 3;          // initialize value to array at given index no
    arr[1] = 4;
    arr[2] = 5;
    arr[3] = 6;
    arr[4] = 7;
    cout<<arr[2]<<endl;
    cout<<arr[1]<<endl;
    cout<<arr[4]<<endl;


    return 0;
}
