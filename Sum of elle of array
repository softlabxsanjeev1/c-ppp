#include <iostream>
using namespace std;
int main() {
    int arr[12];          // decalaration of array

    // loop to take input values and store in array
    for(int i = 0; i<=6; i++){
        cin>>arr[i];
    }

    // lopp for print sum of elements of array
    int sum = 0;
  
    for(int i = 0; i<=6; i++){
       cout<<arr[i]<<" ";
       sum = sum + arr[i];
    }
    cout<<endl;
    cout<<"Sum of elements of array = "<<sum;
    return 0;
}
