#include <iostream>
using namespace std;
int main() {
    int arr[12];          // decalaration of array

    // loop to take input values and store in array
    for(int i = 0; i<=12; i++){
        cin>>arr[i];
    }

    cout<<"Index  "<<"  values"<<endl; 
    
    // lopp for print values of array
    for(int i = 0; i<=12; i++){
            cout<<" "<<i<<"    ->   "<< arr[i]<<endl;
        }
    return 0;
}
